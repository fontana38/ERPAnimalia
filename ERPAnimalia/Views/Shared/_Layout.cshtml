<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Animalias Administración</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/themes/base/css")
   
    @Styles.Render("~/Content/css/gijgo.min.css")
    @Styles.Render("~/Content/css/gijgo.css")
    @Styles.Render("~/Content/bootstrap.min.css")
    @Styles.Render("~/Content/bootstrap-theme.min.css")
    @Styles.Render("~/Content/bootstrap-datepicker.css")
    @Styles.Render("~/Content/bootstrap-datepicker.min.css")
    @Styles.Render("~/Content/jquery-ui.css")
    <script src="~/Scripts/gijgo.min.js"></script>
    <link href="~/Content/css/gijgo.css" rel="stylesheet" />
    <script src="~/Scripts/gijgo.js"></script>
    <link href="~/Content/css/gijgo.min.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/grid/grid.min.js"></script>
    <script src="~/Scripts/grid/grid.js"></script>
    <script src="http://code.gijgo.com/1.3.0/js/gijgo.js" type="text/javascript"></script>
    <link href="~/" rel="stylesheet" type="text/css">
    @*<script src="~/Scripts/jquery-3.1.1.intellisense.js"></script>*@
  
    @*<script src="~/Scripts/moment.js"></script>*@
    <script src="~/Scripts/client.js"></script>
   
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/bootstrap3.3.7.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Scripts/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/bootstrap-datepicker.min.js"></script>
    <script src="~/Scripts/date.format.js"></script>
</head>

<body>
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">WebSiteName</a>
            </div>
            <ul class="nav navbar-nav">
                <li class="active"><a href="#">Home</a></li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Ventas <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Producto","Index","Product")</li>
                        <li>@Html.ActionLink("Cliente","Index", "Cliente")</li>
                        <li>@Html.ActionLink("Factura", "CrearFactura", "Factura")</li>

                    </ul>
                </li>
                <li>
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Compras <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="#">Proveedor</a></li>
                        <li><a href="#">Articulo</a></li>
                        <li><a href="#">Factura</a></li>
                    </ul>
                </li>
                <li><a href="#">Page 2</a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li><a href="#"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
                <li><a href="#"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
            </ul>
            @Html.Partial("_LoginPartial")
        </div>
    </nav>
                     
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")*@
   
</body>
</html>
